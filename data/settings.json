  },
  "updates": {
    "autoCheck": true,
    "checkIntervalDays": 7
  },
  "diagnostics": {
    "enableLogs": false,
    "logLevel": "error"
  },
  "cli": {
    "enableShortcuts": true,
    "defaultStartOption": "main"
  }
