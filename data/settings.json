{
  "user": {
    "githubUsername": "",
    "githubToken": "",
    "autoLogin": false
  },
  "paths": {
    "defaultWorkspace": "",
    "defaultExportPath": "",
    "rememberLastFolder": true
  },
  "behavior": {
    "autoClearTerminal": true,
    "askBeforeDelete": true,
    "confirmBeforeActions": false
  },
